{
  "presets": [["next/babel"]],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "react": "preact/compat",
          "react-dom": "preact/compat"
        }
      }
    ]
  ]
}
